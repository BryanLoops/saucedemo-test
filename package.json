{
  "name": "teste-bix",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run headless || npm run merge || npm run marge || npm run report",
    "merge": "npx mochawesome-merge \"report/json/*.json\" > \"mochawesome-report/merged-output.json\"",
    "marge": "npx marge mochawesome-report/merged-output.json --reportDir mochawesome-report",
    "report": "node \"cypress/support/utils/processResults.js\"",
    "headless-standard": "npx cypress run --spec \"cypress/e2e/standard\" --browser chrome",
    "headless-problem": "npx cypress run --spec \"cypress/e2e/problem\" --browser chrome",
    "headless": "npx cypress run --spec \"cypress/e2e/error\" --browser chrome --reporter mochawesome --reporter-options reportDir=\"report/json\",overwrite=false,html=false,json=true",
    "report:clean": "rm -rf \"report\""
  },
  "author": "Bryan Lopes",
  "license": "ISC",
  "dependencies": {
    "archiver": "^7.0.1",
    "cypress-axe": "^1.5.0",
    "eslint-plugin-cypress": "^3.5.0",
    "mocha-json-reporter": "^1.0.0-1",
    "nodemailer": "^6.9.16",
    "teste-bix": "file:"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "cypress": "^13.14.2",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
